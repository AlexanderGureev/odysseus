{
  "name": "odysseus-player",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npx nodemon",
    "build:client": "NODE_ENV=production webpack --config webpack.prod.config.ts",
    "build:server": "NODE_ENV=production webpack --config webpack.server.config.ts",
    "build:live": "NODE_ENV=production webpack --config ./live/config/webpack.client.js --mode production",
    "build": "npx rimraf build && npx concurrently \"npm run build:client\"  \"npm run build:server\" \"npm run build:live\"",
    "start:prod": "node -r dotenv/config ./build/server/index.js"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "build",
      "node_modules",
      "src"
    ],
    "watch": [
      "server"
    ],
    "exec": "webpack --config webpack.server.config.ts && node -r dotenv/config ./build/server/index.js",
    "ext": "ts, tsx"
  },
