    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.9",
    "@types/copy-webpack-plugin": "^6.4.0",
    "@types/css-minimizer-webpack-plugin": "^1.1.3",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/jest": "^26.0.22",
    "@types/md5": "^2.3.0",
    "@types/mini-css-extract-plugin": "^1.2.2",
