    "express": "^4.17.1",
    "jest": "^26.6.3",
    "just-detect-adblock": "^1.1.0",
    "md5": "^2.3.0",
    "pug": "^3.0.2",
    "randomstring": "^1.1.5",
    "react": "^17.0.1",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.0",
    "react-yandex-metrika": "^2.6.0",
    "request-ip": "^2.1.3",
    "shaka-player": "^3.1.0",
    "uuid": "^8.3.2",
    "video.js": "^7.11.8",
    "videojs-contrib-eme": "^3.8.0"
  },
