{
  "name": "odysseus-player",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npx nodemon",
    "build:client": "NODE_ENV=production webpack --config webpack.prod.config.ts",
    "build:server": "NODE_ENV=production webpack --config webpack.server.config.ts",
    "build": "npx rimraf build && npx concurrently \"yarn build:client\"  \"yarn build:server\""
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "build",
      "node_modules",
      "src"
    ],
    "watch": [
      "server"
    ],
    "exec": "webpack --config webpack.server.config.ts && node -r dotenv/config ./build/server/index.js",
    "ext": "ts, tsx"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
